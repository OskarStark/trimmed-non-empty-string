{
    "name": "oskarstark/trimmed-non-empty-string",
    "type": "library",
    "description": "This library provides a value object which ensures a trimmed non empty string.",
    "keywords": [
        "string",
        "value-object"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/string": "^6.0 || ^7.0 || ^8.0",
        "webmozart/assert": "^1.11 || ^2.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "ergebnis/data-provider": "^3.2",
        "fakerphp/faker": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.70",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "autoload": {
        "psr-4": {
            "OskarStark\\Value\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OskarStark\\Value\\Tests\\": "tests/"
        }
    }
}
